{
    "name" : "artscorestudio/contact-bundle",
    "license" : "MIT",
    "type" : "library",
    "description" : "Contact Bundle is a Symfony 2/3 bundle for create and manage contacts in your Symfony 2/3 application. This package is a part of Artscore Studio Framework.",
    "homepage" : "https://github.com/artscorestudio/contact-bundle",
    "authors" : [{
            "name" : "Nicolas Claverie",
            "email" : "info@artscore-studio.fr"
        }, {
            "name" : "Artscore Studio",
            "homepage" : "http://www.artscore-studio.fr"
        }
    ],
    "require" : {
        "php" : ">=5.3.9",
        "symfony/symfony" : "~2.8|~3.0",
        "artscorestudio/datagrid-bundle" : "^3.0"
    },
    "require-dev" : {
        "hautelook/alice-bundle" : "^1.3",
        "doctrine/data-fixtures" : "^1.2"
    },
    "suggest" : {
        "artscorestudio/layout-bundle" : "A Symfony 2/3 bundle for manager User Interface."
